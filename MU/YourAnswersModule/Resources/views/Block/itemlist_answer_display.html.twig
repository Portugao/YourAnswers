{# Purpose of this template: Display answers within an external context #}
{% for answer in items %}
    <h3>{{ answer|muyouranswersmodule_formattedTitle }}</h3>
    <p><a href="{{ path('muyouranswersmodule_answer_display', {'id': answer.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
